<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	
	<servlet>
		<servlet-name>LoginSome</servlet-name>
		<servlet-class>controller.baseC.LoginSome</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginSome</servlet-name>
		<url-pattern>*.login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Plan</servlet-name>
		<servlet-class>controller.clientC.Plan</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Plan</servlet-name>
		<url-pattern>*.plan</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Doing</servlet-name>
		<servlet-class>controller.clientC.Doing</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Doing</servlet-name>
		<url-pattern>*.doing</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Myself</servlet-name>
		<servlet-class>controller.baseC.Myself</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Myself</servlet-name>
		<url-pattern>*.myself</url-pattern>
	</servlet-mapping>
	
	
</web-app>